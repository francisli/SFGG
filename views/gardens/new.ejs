<h1>New Garden</h1>
<form method="post" action="/gardens">
  <div class="form-group">
    <label>Garden Name</label>
    <input type="text" class="form-control" name="name">
  </div>

  <div class="form-group">
    <label>Short Description</label>
    <textarea class="form-control" name="shortDescription">
    </textarea>
  </div>

  <div class="form-group">
    <label>Photo URL</label>
    <input type="text" class="form-control" name="photoUrl">
  </div>

  <div class="form-group">
    <label>Address</label>
    <input type="text" class="form-control" name="address">
  </div>

  <div class="form-group">
    <label>Cross Street</label>
    <input type="text" class="form-control" name="crossStreet">
  </div>

  <div class="form-group">
    <label>City</label>
    <input type="text" class="form-control" name="city">
  </div>

  <div class="form-group">
    <label>Zip Code</label>
    <input type="text" class="form-control" name="zipCode">
  </div>

  <div class="form-group">
    <label>Neighborhood</label>
    <input type="text" class="form-control" name="neighborhood">
  </div>

  <div class="form-group">
    <label>Managed By</label>
    <input type="text" class="form-control" name="managedBy">
  </div>

  <div class="form-group">
    <label>Open Hours</label>
    <input type="text" class="form-control" name="openHours">
  </div>

  <div class="form-group">
    <label>Contact Email</label>
    <input type="text" class="form-control" name="contactEmail">
  </div>

  <div class="form-group">
    <label>Contact Phone</label>
    <input type="text" class="form-control" name="contactPhone">
  </div>

  <div class="form-group">
    <label>Contact Link</label>
    <input type="text" class="form-control" name="contactLink">
  </div>

  <div class="form-group">
    <button class="btn btn-primary" type="submit">Submit</button>
  </div>

</form>
